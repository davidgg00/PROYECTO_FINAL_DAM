<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.AutoBurger.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.AutoBurger.app.ActivityVerHistorialPedidos"></activity>
        <activity android:name="com.AutoBurger.app.EsperarComida" />
        <activity android:name="com.AutoBurger.app.VerPedidoActual" />
        <activity android:name="com.AutoBurger.app.VerDetallesMenu" />
        <activity android:name="com.AutoBurger.app.VerDetallesProducto" />
        <activity android:name="com.AutoBurger.app.FormularioRegistro" />
        <activity android:name="com.AutoBurger.app.ActivityCartaAlimentos" />
        <activity android:name="com.AutoBurger.app.BienvenidaActivity" />
        <activity android:name="com.AutoBurger.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>